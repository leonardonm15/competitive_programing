#include <bits/stdc++.h>

using namespace std;

#define endl '\n' 
//#define int long long

// ----------    GRIND MENTALITY    ---------     
// /* ESCREVE TODAS AS SUAS IDEIAS E TESTA */
// ----------    GRIND MENTALITY    ---------     

vector<int> c = {15, 10, 6, 3, 1};
void solve () {
    int n; cin >> n;
    vector<int> dp(17);
    for (auto cara: c) dp[cara]++;
    int lc = 1;
    for (int i=2; i < dp.size(); i++) {
        if (dp[i] == 1) lc = i;
        dp[i] = dp[lc] + dp[i - lc];
    }


    cout << "dp -> ";
    for (auto cara: dp) cout << cara << " ";
    cout << endl;
    int resto = (n % 15);
    int nm = n / 15;
    cout << "resto -> " << resto << endl;
    /* cout << "nm -> " << nm << endl; */

    cout << nm + dp[resto] << endl;
}

signed main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    int TC = 1;
    if (TC){
        cin >> TC;
        while (TC--) solve();
    } else {
        solve();
    }
    return 0;
}
