void solve () {
    // sorta o de baixo e guarda os indices e e vai colocando a resp no array 
    int n, q; cin >> n >> q;
    int cum = 0;
    vector<int> pref(n + 1);
    for (int i=0; i < n; i++) {
        int num; cin >> num;
        cum += num;
        arr[i + 1] = cum;
    }
    for (int i=0; i < q; i++) {
        int num; cin >> num;
        int mid, l, r;
        l = 0;
        r = n;
        mid = (l + r) / 2;
        while (l <= r) {
            if (pref[mid] > num) {
                r = mid - 1;
            } else if (pref[mid] <= num) {
                l = mid;
            }
            mid = (l + r) / 2;
        }
    }
}

signed main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    int TC = 1;
    if (TC){
        cin >> TC;
        while (TC--) solve();
    } else {
        solve();
    }
    return 0;
}
