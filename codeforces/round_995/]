void solve () {
    int n, m, q; cin >> n >> m >> q;
    cuts.resize(q);

    map<int, int> pos;
    pos[m]++;

    for (int i = 0; i < q; i++) cin >> cuts[i];


}

signed main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    int TC = 1;
    if (TC){
        cin >> TC;
        while (TC--) solve();
    } else {
        solve();
    }
    return 0;
}
