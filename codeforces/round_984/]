#include <bits/stdc++.h>

using namespace std;

#define endl '\n' 
#define int long long

void solve () {
    int n, k; cin >> n >> k;

    vector<int> brand(1005);

    for (int i = 0; i < k; i++) {
        int b, c; cin >> b >> c;
        brand[b] += c;
    }

    sort(brand.rbegin(), brand.rend());

    int resp = 0;
    for (int i = 0; i < n; i++) {
        resp += brand[i];
    }
}

signed main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    int TC = 1;
    if (TC){
        cin >> TC;
        while (TC--) solve();
    } else {
        solve();
    }
    return 0;
}
