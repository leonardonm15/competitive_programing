#include <bits/stdc++.h>
// https://codeforces.com/problemset/problem/1676/G

using namespace std;
//#define int long long
const int maxn = 1e5 + 5;
int n;
vector<int> adj[maxn];
int color[maxn];
int dp[maxn][2];

void solve () {
    cin >> n;
    for (int i=2; i <= n; i++) {
        int num; cin >> num;
        adj[num].push_back(i);
    }
    
}

signed main() {
    ios_base::sync_with_stdio(0);cin.tie(0);
    int TC = 1;
    if (TC){
        cin >> TC;
        while (TC--) solve();
    } else {
        solve();
    }
    return 0;
}
